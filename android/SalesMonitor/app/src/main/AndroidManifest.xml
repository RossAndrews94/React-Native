<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.salesmonitor">

    <application
        android:name="com.example.android.salesmonitor.SalesMonitorApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.salesmonitor.activitiy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.android.salesmonitor.activitiy.MainMenuActivity" />
        <activity android:name="com.example.android.salesmonitor.activitiy.SaveSaleActivity" />
        <activity android:name="com.example.android.salesmonitor.activitiy.ShowAllActivity" />
        <activity android:name="com.example.android.salesmonitor.activitiy.FirebaseAuthActivity" />
        <activity android:name="com.example.android.salesmonitor.activitiy.AddShopActivity" />
        <activity android:name="com.example.android.salesmonitor.activitiy.ShowAllClientsActivity"></activity>
    </application>

</manifest>